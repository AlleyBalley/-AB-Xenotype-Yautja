						<hediff>ABYautjaUpperMandibles</hediff>
						<bodyparts>
							<li>Jaw</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>	
    </GeneDef>
	
