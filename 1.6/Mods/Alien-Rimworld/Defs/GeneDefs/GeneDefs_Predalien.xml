<!--Queen-->
	<GeneDef>
		<defName>ABQueenPredalien_Head</defName>
		<geneClass>Xenomorphtype.XenomorphGene</geneClass>
		<label>Queen Predalien Head</label>
		<displayCategory>ABYautjaCategory</displayCategory>
		<description>Carriers of this gene sport mandibles and an odd reflective head-casing.</description>
		<iconPath>UI/Icons/Genes/Gene_PredalienHead</iconPath>
		<biostatCpx>1</biostatCpx>
		<biostatMet>0</biostatMet>
		<canGenerateInGeneSet>false</canGenerateInGeneSet>
		<selectionWeight>1</selectionWeight>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPath>Things/Pawn/Humanlike/Predalien/HeadAttachments/PredalienQueenFace/PredalienQueenFace</texPath>
				<parentTagDef>Head</parentTagDef>
				<colorType>Skin</colorType>
				<useRottenColor>true</useRottenColor>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<visibleFacing>
					<li>East</li>
					<li>South</li>
					<li>West</li>
				</visibleFacing>
				<baseLayer>85</baseLayer>
			</li>
		</renderNodeProperties>
	</GeneDef>
	
