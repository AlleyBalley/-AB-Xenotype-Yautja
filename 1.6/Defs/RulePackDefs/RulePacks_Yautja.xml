<!--Yautja Unisex Rulepack-->
	<RulePackDef>
		<defName>ABNamerPerson_Yautja_Unisex</defName>
		<include>
			<li>NamerUtility_ABYautja</li>
			<li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[ABUnisexfirstname] [ABUnisexlastname]</li>
				<li>name->[ABUnisexfirstname] '[nickname]' [ABUnisexlastname]</li>
				<li>name->[ABYautjaShortName] '[nickname]' [ABUnisexlastname]</li>
				<li>name->[ABYautjaShortName] [ABUnisexlastname]</li>
				
				<li>ABUnisexfirstname(p=2)->[ABNameYautjaUnisexA][ABNameYautjaUnisexB]</li>
				<li>ABUnisexfirstname(p=2)->[ABNameYautjaUnisexA][ABNameYautjaUnisexB]</li>
				<li>ABUnisexfirstname(p=3)->[ABNameYautjaUnisexA][ABNameYautjaUnisexB][AbNameYautjaUnisexEnd]</li>
				<li>ABUnisexfirstname(p=2)->[ABNameYautjaUnisexA][ABNameYautjaUnisexB][ABNameYautjaUnisexB]</li>
				<li>ABUnisexfirstname(p=3)->[ABNameYautjaUnisexA][ABNameYautjaUnisexB][ABNameYautjaUnisexB][AbNameYautjaUnisexEnd]</li>
				<li>ABUnisexfirstname(p=3)->[ABNameYautjaUnisexA][AbNameYautjaUnisexEnd]</li>
				<li>ABYautjaShortName(p=6)->[ABNameYautjaShortName]</li>
				
				<li>ABUnisexlastname(p=3)->[WordYautji]</li>
				<li>ABUnisexlastname(p=2)->[WordYautji]</li>
				
				<li>nickname(p=8)->[ABNameYautjaUnisexNick]</li>
				<li>nickname(p=8)->[ABNameYautjaAdj] [ABNameYautjaUnisexNick]</li>
				<li>nicktname(p=3)->[ConceptBadass]</li>
				<li>nicktname(p=2)->[PersonBadass]</li>
				<li>nicktname(p=2)->[AnimalBadass]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
