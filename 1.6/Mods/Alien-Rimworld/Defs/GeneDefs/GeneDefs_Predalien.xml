<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!--Drone-->
	<GeneDef>
		<defName>ABPredalien_Head</defName>
		<geneClass>Xenomorphtype.XenomorphGene</geneClass>
		<label>Predalien Head</label>
		<displayCategory>ABYautjaCategory</displayCategory>
		<description>Carriers of this gene have long elongated heads with a pronounced ridge along the top.</description>
		<iconPath>UI/Icons/Genes/Gene_PredalienHead</iconPath>
		<biostatCpx>1</biostatCpx>
		<biostatMet>0</biostatMet>
		<canGenerateInGeneSet>false</canGenerateInGeneSet>
		<selectionWeight>1</selectionWeight>
		<forcedHeadTypes>
			<li>ABPredalien_Head</li>
		</forcedHeadTypes>
	</GeneDef>
	
	<GeneDef ParentName="ABYautjaMandibles_Base">
        <defName>ABPredalienUpperMandibles</defName>
        <label>Predalien Superior Upper Mandibles</label>
        <description>A finger-like digit that sprouts from the outer cheekbone and possesses a sharp tooth.</description>
        <iconPath>UI/Icons/Genes/GeneIcon_YautjaSuperiorUpperMandibles</iconPath>
        <renderNodeProperties>
			<li>
				<texPath>Things/Pawn/Humanlike/Predalien/HeadAttachments/PredalienUpperMandibles/ABPredalienUpperMandibles</texPath>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<parentTagDef>Head</parentTagDef>
				<color>(255, 255, 255)</color>
				<visibleFacing>
					<li>East</li>
					<li>South</li>
					<li>West</li>
				</visibleFacing>
				<baseLayer>58</baseLayer>
			</li>
        </renderNodeProperties>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>mandible</symbol>
				</li>
				<li>
					<symbol>saber</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>	
		<modExtensions MayRequire="oskarpotocki.vanillafactionsexpanded.core">
			<li Class="VEF.Genes.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Yautja_Endogene_Background</backgroundPathEndogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>ABYautjaSuperiorUpperMandibles</hediff>
						<bodyparts>
							<li>Jaw</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>	
    </GeneDef>
	
