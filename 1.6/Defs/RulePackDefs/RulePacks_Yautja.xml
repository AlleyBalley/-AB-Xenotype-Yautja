<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!--Yautja Utility Rulepack-->
  <RulePackDef>
    <defName>NamerUtility_ABYautja</defName>
    <rulePack>
		<rulesStrings>
			<li>WordYautji(p=3)->[SylG]'[SylE]-[SylG]</li>
			<li>WordYautji(p=2)->[SylG]'[SylG][SylE]-[SylG]</li>
		</rulesStrings>
		<rulesFiles>
			<li>ABNameYautjaShortName->NameBanks/ABYautja_ShortName</li>
		
			<li>ABNameYautjaUnisexA->NameBanks/ABYautja_UnisexA</li>
			<li>ABNameYautjaUnisexB->NameBanks/ABYautja_UnisexB</li>
			
			<li>ABNameYautjaFemEnd->NameBanks/ABYautja_FemEnd</li>
			<li>ABNameYautjaMascEnd->NameBanks/ABYautja_MascEnd</li>
			<li>ABNameYautjaUnisexEnd->NameBanks/ABYautja_UnisexEnd</li>
			
			<li>ABNameYautjaAdj->NameBanks/ABYautja_Adj</li>
			<li>ABNameYautjaUnisexNick->NameBanks/ABYautja_Nick</li>
			
			<li>SylE->WordParts/Syllables_English</li>
			<li>SylG->WordParts/Syllables_Galician</li>
		</rulesFiles>
    </rulePack>
  </RulePackDef>
<!--Yautja Faction Rulepack-->
	<RulePackDef>
		<defName>NamerFaction_ABYautja</defName>
		<include>
			<li>ABNamerYautjaBases</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[ABYautja_Clans] [ABBaseNames]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>ABYautja_Clans</keyword>
					<path>NameBanks/ABYautja_Clans</path>
				</li>
			</rulesRaw>
		</rulePack>
	</RulePackDef>
